{
  "name": "@binance-chain/javascript-sdk",
  "version": "2.6.0",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest",
    "test:ledger": "qunit --require babel-polyfill tests-ledger/tests.js",
    "test:ledger:browser": "./tests-ledger/run-browser-tests.sh",
    "build": "rm -rf lib && npx babel src -d lib",
    "build:docs": "npx jsdoc-to-markdown -f src/**/*.js > docs/jsdoc.md",
    "prepublishOnly": "npm run build",
    "lint": "eslint --fix src __tests__"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "base32-encode": "^1.1.0",
    "bech32": "^1.1.3",
    "big.js": "^5.2.2",
    "bip32": "^1.0.2",
    "bip39": "^2.5.0",
    "bn.js": "^4.11.8",
    "crypto-browserify": "^3.12.0",
    "crypto-js": "^3.1.9-1",
    "lodash": "^4.17.11",
    "safe-buffer": "^5.1.2",
    "secure-random": "^1.1.1",
    "tiny-secp256k1": "^1.0.1",
    "uuid": "^3.3.2",
    "varstruct": "^6.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "8.2.6",
    "babel-jest": "22.4.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react-native": "4.0.0",
    "babel-preset-stage-2": "^6.24.1",
    "browserify": "^16.2.3",
    "buffer": "^5.2.1",
    "cross-env": "^5.2.0",
    "eslint": "4.19.1",
    "http-server": "^0.11.1",
    "jest": "22.4.3",
    "jsdoc-to-markdown": "^4.0.1",
    "qunit": "^2.9.1"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "files": [
    "lib/*"
  ]
}